{
  "npmScope": "wishlist",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "wishlist-desktop-app": {
      "tags": ["scope:wishlist-desktop-app"]
    },
    "wishlist-desktop-app-e2e": {
      "tags": []
    },
    "ui": {
      "tags": ["scope:shared-ui"]
    },
    "ui-e2e": {
      "tags": [],
      "implicitDependencies": ["ui"]
    },
    "mylist-shared-data-access": {
      "tags": ["scope:mylist-infrastructure"]
    },
    "mylist-feature-shell": {
      "tags": ["scope:mylist-feature"]
    },
    "shared-domain": {
      "tags": ["scope:shared-domain"]
    },
    "mylist-shared-store": {
      "tags": ["scope:mylist-infrastructure"]
    }
  }
}
